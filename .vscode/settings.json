{
    "eslint.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.packageManager": "yarn",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "vue",
            "autoFix": true
        }
    ],
    "emmet.syntaxProfiles": {
        "vue": "css"
    },
    "files.associations": {
        "*.vue": "vue"
    }
}